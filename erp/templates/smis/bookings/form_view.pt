<!-- top menu -->
<div class="top">
    <div class="row">
        <div class="col-lg-2">
            <button type="submit" class="btn btn-primary btn-block" id="submit" name="submit" data-gritter-title="Quotation Entry">
                <i class="fa fa-save"></i> SAVE
            </button>
        </div>
        <div class="col-lg-10">
            <div class="top-menu">
                <ul class="list-inline toggle-menu">
                    <li>
                        <a href="${request.route_url('action', action='create', module='smis', cls='bookings')}"
                           class="btn btn-default">
                            <i class="fa fa-pencil"></i>CREATE NEW
                        </a>
                    </li>
                </ul>
                <ul class="list-inline pull-right">
                    <li class="cancel">
                        <a class="btn btn-default" href="${request.route_url('index', module='smis', cls='bookings')}">
                            <i class="fa fa-close"></i> CANCEL
                        </a>
                    </li>
                </ul>
                <div class="navigation">
                    <button type="button" class="btn btn-link hidden-sm hidden-md hidden-lg inbox-nav-toggle">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /top menu -->
<!-- bottom -->
<div class="bottom">
    <div class="row">
        <!-- inbox left menu -->
        <div class="col-xs-12 col-sm-3 col-lg-2 inbox-left-menu">
            <h3 class="sr-only">Inbox Menu</h3>
            <ul class="list-unstyled left-menu">
                <li class="active">
                    <a href="#tab-main" data-toggle="tab">
                        <i class="fa fa-folder-open"></i> Booking Information
                    </a>
                </li>
                <li>
                    <a href="#tab-remarks" data-toggle="tab">
                        <i class="fa fa-folder-o"></i> Remarks
                    </a>
                </li>
                <li>
                    <a href="#tab-buying" data-toggle="tab">
                        <i class="fa fa-folder-o"></i> Buying Rate
                    </a>
                </li>
                <li>
                    <a href="#tab-commission" data-toggle="tab">
                        <i class="fa fa-folder-o"></i> Shipper's Commission
                    </a>
                </li>
                <li>
                    <a href="#tab-charges" data-toggle="tab">
                        <i class="fa fa-folder-o"></i> Charges
                    </a>
                </li>
            </ul>
        </div>
        <!-- end inbox left menu -->
        <!-- right main content, new message form -->
        <div class="col-xs-12 col-sm-9 col-lg-10">
            <div class="new-message-form">
                <div class="tab-content">
                    <div id="tab-main" role="tabpanel" class="tab-pane active">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Booking No</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Autogenerated" readonly>
                            </div>
                            <label class="col-sm-2 control-label">Job Order No</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Enter JO #">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Service Type</label>
                            <div class="col-sm-4">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Select Service Type</option>
                                    <option>Export</option>
                                    <option>Import</option>
                                </select>
                            </div>
                            <label class="col-sm-2 control-label">Integrated Service</label>
                            <div class="col-sm-4">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Select Service</option>
                                    <option>Brokerage</option>
                                    <option>Moving</option>
                                </select>
                            </div>
                            <div class="col-sm-12">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <input type="checkbox" id="Nominated">
                                    <label for="Nominated">Nominated</label>
                                </div>
                                <div class="col-sm-4 col-sm-offset-2">
                                    <input type="checkbox" id="Separate">
                                    <label for="Nominated">Separate Billing</label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Account</label>
                            <div class="col-sm-6">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Select Account Officer</option>
                                    <option>Employee 1</option>
                                    <option>Employee 2</option>
                                </select>
                            </div>
                            <label class="col-sm-2 control-label">Date</label>
                            <div class="col-sm-2">
                                <input type="text" class="form-control datetimepicker valid" placeholder="MM/DD/YYYY">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Shipper/Consignee</label>
                            <div class="col-sm-6">
                                <input type="text" class="form-control" placeholder="Select Company">
                            </div>
                            <label class="col-sm-2 control-label">Classification</label>
                            <div class="col-sm-2">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Select Class</option>
                                    <option>New</option>
                                    <option>Old</option>
                                    <option>JS</option>
                                    <option>Reg</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Contact Person</label>
                            <div class="col-sm-6">
                                <input type="text" class="form-control" placeholder="Select Contact Person">
                            </div>
                            <label class="col-sm-2 control-label">Credit Terms</label>
                            <div class="col-sm-2">
                                <input type="text" class="form-control" placeholder="0">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Origin</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Origin">
                            </div>
                            <label class="col-sm-2 control-label">Airline</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Airline">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Destination</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Destination">
                            </div>
                            <label class="col-sm-2 control-label">Forwarder</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Forwarder">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Freight terms</label>
                            <div class="col-sm-4">
                                <select class="form-control">
                                    <option>Freight Collect</option>
                                    <option>Freight Prepaid</option>
                                    <option>Ex-works Collect</option>
                                    <option>Ex-works Prepaid</option>
                                    <option>Airport-to-Door</option>
                                    <option>Door-to-Door</option>
                                </select>
                            </div>
                            <label class="col-sm-2 control-label">Commodity</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Commodity">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Dimension</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Dimension">
                            </div>
                            <label class="col-sm-2 control-label">Pkgs/Weight</label>
                            <div class="col-sm-2">
                                <input type="text" class="form-control" placeholder="Pkgs">
                            </div>
                            <div class="col-sm-2">
                                <input type="text" class="form-control" placeholder="Weight">
                            </div>
                        </div>
                        <hr>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Noted by</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" name="noted" placeholder="Noted by">
                            </div>
                            <label class="col-sm-2 control-label">Status</label>
                            <div class="col-sm-4">
                                <select class="form-control">
                                    <option>Draft</option>
                                    <option>Confirmed</option>
                                    <option>Cancelled</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Approved by</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" name="approve" placeholder="Approved by">
                            </div>
                        </div>
                    </div>
                    <div id="tab-remarks" role="tabpanel" class="tab-pane">
                        <div class="col-sm-12">
                            ${form.hidden("remarks")}
                            <div data-input="#remarks" tal:content="structure form.value('remarks')">
                            </div>
                            <script type="text/javascript">
                                if (window['summernote_remarks'] !== undefined) {
                                    window['summernote_remarks']();
                                }
                            </script>
                        </div>
                    </div>
                    <div id="tab-buying" role="tabpanel" class="tab-pane">
                        <div class="form-group">
                            <div class="col-sm-2">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Type</option>
                                </select>
                            </div>

                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Description">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Currency</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <input type="text" class="col-sm-2 form-control" placeholder="Rate">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Unit</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div id="tab-commission" role="tabpanel" class="tab-pane">
                        <div class="form-group">
                            <div class="col-sm-2">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Type</option>
                                </select>
                            </div>

                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="Company/Personnel">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Currency</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <input type="text" class="col-sm-2 form-control" placeholder="Rate">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Unit</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div id="tab-charges" role="tabpanel" class="tab-pane">
                        <div class="form-group">
                            <div class="col-sm-2">
                                <select class="form-control">
                                    <option disabled="disabled" selected="selected" value="">Group</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <input type="text" class="form-control" placeholder="Description">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Currency</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <input type="text" class="col-sm-2 form-control" placeholder="Rate">
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Unit</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <select class="col-sm-2 form-control">
                                    <option disabled="disabled" selected="selected" value="">Terms</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                ${form.hidden("id")}
                <!-- load erp:templates/form_errors.pt -->
            </div>
        </div>
        <!-- end right main content, new message form -->
    </div>
</div>
<!-- /bottom -->
